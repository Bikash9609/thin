{
  "name": "Thin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:emulator": "node runEmulator.js",
    "android": "yarn android:emulator && react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "update:version": "npx react-native-version --never-amend",
    "build": "yarn android:emulator && npx react-native@latest build-android --mode=release && ( [ \"$(basename $PWD)\" != \"android\" ] && cd ./android ; ./gradlew assembleRelease )",
    "generate:bootsplash": "yarn react-native generate-bootsplash assets/icon.png --platforms=android,ios,web --background=F5FCFF --logo-width=100 --assets-output=assets --flavor=main --html=index.html"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.2",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-native-firebase/app": "^20.1.0",
    "@react-native-firebase/auth": "^20.1.0",
    "@react-native-firebase/messaging": "^20.1.0",
    "@react-native-google-signin/google-signin": "^12.1.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "add": "^2.0.6",
    "axios": "^1.6.7",
    "formik": "^2.4.6",
    "lottie-react-native": "^6.6.0",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-bootsplash": "^5.4.1",
    "react-native-compressor": "^1.8.24",
    "react-native-device-info": "^10.13.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-image-viewing": "^0.2.2",
    "react-native-permissions": "^4.1.5",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-share": "^10.0.2",
    "react-native-size-matters": "^0.4.2",
    "react-native-snackbar": "^2.6.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-view-shot": "^3.8.0",
    "sp-react-native-in-app-updates": "^1.4.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-snap-carousel": "^3.8.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.56.0",
    "jest": "^29.6.3",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
